{
		"name": "webdevstudios/wds-coding-standards",
		"description": "In-house linting and coding standards for WebDevStudios",
		"type": "library",
		"minimum-stability": "stable",
		"authors": [],
		"require": {
				"wp-coding-standards/wpcs": "2.1.0",
				"squizlabs/php_codesniffer": "^3.3.1"
		},
		"require-dev": {
				"dealerdirect/phpcodesniffer-composer-installer": "^0.5"
		},
		"scripts": {
			"post-install-cmd": "npm install -g \"$HOME/.composer/vendor/webdevstudios/wds-coding-standards/WebDevStudios/eslint-plugin-webdevstudios\"",
			"post-update-cmd": "npm install -g \"$HOME/.composer/vendor/webdevstudios/wds-coding-standards/WebDevStudios/eslint-plugin-webdevstudios\""
		}
}
